#!/usr/bin/env bash

echo -en "\tFinished loading at "
date

inserted=$(curl -u "elastic:elasticsearch_pwd" -s -X GET "localhost:9200/$test/_count" | jq .count)
echo -e "\tCount of inserted documents: $inserted"

if [[ ! $inserted -eq 116543900 ]]; then
  >&2 echo "Inserted count mismatch ($inserted / 116543900)"
  exit 98
fi
